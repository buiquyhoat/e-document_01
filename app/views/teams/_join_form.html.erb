<div id="join_member_form">
  <% if @member.present? %>
    <%= render partial: "leave", locals: {
      organization: @organization,
      team: @team,
      member: @member,
      support: @support} %>
  <% else %>
    <%= render partial: "join", locals: {
      organization: @organization,
      team: @team,
      support: @support} %>
  <% end %>
</div>
